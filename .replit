run = "mcs -out:Main.exe 'clinica.cs' 'arquivo.cs' 'Cliente.cs' 'Consulta.cs' 'Gato.cs' 'ItemConsulta.cs' 'Servico.cs' 'Usuario.cs' 'NCliente.cs' 'NConsulta.cs' 'NGato.cs' 'NItemConsulta.cs' 'NServico.cs' 'NUsuario.cs' ; mono Main.exe"

hidden = ["bin", "obj"]

[env]
DOTNET_NOLOGO = "1"
DOTNET_CLI_TELEMETRY_OPTOUT = "1"

[packager]
language = "dotnet"

[packager.features]
packageSearch = true

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[nix]
channel = "stable-21_11"

[languages.csharp]
pattern = "**/*.cs"

  [languages.csharp.languageServer]
  start = ["omnisharp", "--languageserver"]
